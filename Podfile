target 'SocialNetworkShare' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
   pod ‘TwitterKit’
   pod ‘Fabric’
   pod ‘SwiftyJSON’
   pod ‘PinterestSDK’
   pod 'TMTumblrSDK'
   pod 'Toast-Swift', '~> 2.0.0'
  #Pods for SocialNetworkShare

end
